<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Suite"
  thread-count="2" 
  parallel="tests" verbose="4">
  
  
    <listeners>
			<listener
		class-name="com.qa.opencart.listeners.AnnotationTransformer" />
